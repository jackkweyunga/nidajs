{
  "name": "nidjs",
  "version": "0.0.0-development",
  "description": "A node module to fetch details for a given NIN (tz)",
  "main": "./dist/nid.js",
  "types": "./dist/nid.d.ts",
  "scripts": {
    "test": "jest",
    "commit": "git add . && git cz",
    "semantic-release": "semantic-release --branches main",
    "build": "npm run build:web && npm run build:node",
    "build:web": "webpack --config webpack.config.js",
    "build:node": "cd node && webpack --config webpack-node.config.js",
    "publish:Web":"npm run build:web && npm publish",
    "publish:node":"npm run build:node && npm publish ./node",
    "publish":"npm run publish:web && npm run publish:node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackkweyunga/nidajs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nidajs",
    "nida npm",
    "nida node",
    "Nida Tanzania",
    "nida",
    "Nida",
    "jackkweyunga",
    "Jack Kweyunga"
  ],
  "author": "Jack Kweyunga @jackkweyunga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackkweyunga/nidajs/issues"
  },
  "homepage": "https://github.com/jackkweyunga/nidajs#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "semantic-release-cli": "^5.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/jest": "^27.0.1",
    "babel-loader": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^27.0.6",
    "semantic-release": "^17.4.5",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "xhr2": "^0.2.1",
    "xmlhttprequest": "^1.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
